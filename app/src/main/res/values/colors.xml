    <color name="textColor">#f7f2f2</color>
    <color name="textColorSecondary">#99f7f2f2</color>
    <color name="textColorMuted">#4df7f2f2</color>
    <color name="textBarColor">#f6eded</color>
    <color name="window_background">#3b6eea</color>
   
    <!-- background drawable: be sure it has a not too large size/resolution -->
    <drawable name="activity_background_image">@drawable/jpg_nostalgicbluebackground06hdpictures169782646</drawable>
    
